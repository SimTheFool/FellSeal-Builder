{
  "extends": "./tsconfig.default.json",
  "compilerOptions": {
    "outDir": "build-test",
    "paths": {
      "@utils/*": ["./utils/*"],
      "@domain/*": ["./domain/*"],
      "@adapters/*": ["./adapters/test/*"],
      "@fixtures/*": ["../fixtures/*"]
    }
  },
  "include": ["./src/**/*.spec.ts"],
  "tsc-alias": {
    "verbose": true,
    "resolveFullPaths": true
  }
}
